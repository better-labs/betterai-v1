{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "prisma generate && next build",
    "dev": "next dev",
    "db:reset": "bash -c 'if [ \"$NODE_ENV\" = \"production\" ]; then echo \"Refusing to run db:reset in production.\" >&2; exit 1; fi; prisma migrate reset --force --skip-seed --skip-generate'",
    "db:seed": "tsx scripts/2-update-polymarket-data.ts && tsx scripts/3-generate-market-research.ts && tsx scripts/4-generate-batch-predictions.ts",
    "db:bootstrap": "pnpm db:reset && pnpm db:seed",
    "data:pipeline": "tsx scripts/5-run-data-pipeline.ts",
    
    "lint": "next lint", 
    "start": "next start",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "cron:update-polymarket-data": "node scripts/cron/cron-update-polymarket-data.js",
    "cron:generate-batch-predictions": "node scripts/cron/cron-generate-batch-predictions.js",
    "cron:prediction-check": "node scripts/cron/cron-prediction-check.js",
    "cron:update-ai-models": "node scripts/cron/cron-update-ai-models.js",
    "test:crons": "pnpm cron:update-polymarket-data && pnpm cron:generate-batch-predictions && pnpm cron:prediction-check",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "bash -c 'DATABASE_URL=\"$DATABASE_URL_UNPOOLED\" prisma migrate deploy'",
    "migrate:deploy:dev": "bash -lc 'set -a; source .env; set +a; pnpm prisma:migrate:deploy'",
    "migrate:deploy:prod": "bash -lc 'set -a; source .env.prod; set +a; pnpm prisma:migrate:deploy'",
    "prisma:studio": "prisma studio",
    "postinstall": "prisma generate"
  },
